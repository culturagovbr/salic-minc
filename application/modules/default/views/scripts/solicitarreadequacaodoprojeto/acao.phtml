	/**
	 * Fun��o respons�vel por fazer a soma dos ingressos e calcular o total
	 */
	function somaIngressos()
	{
		/* ========== INÍCIO SOMA DE INGRESSOS ========== */
		campo1 = document.getElementById('Patrocinador').value;
		campo1 = campo1.replace(/\D/g, ""); // retira os pontos e as v�rgulas, deixando somente n�meros
		campo1 = campo1.replace(/(\d{0})(\d)/, "$1$2");
		campo1 = parseInt(campo1); // converte para int

		campo2 = document.getElementById('Divulgacao').value;
		campo2 = campo2.replace(/\D/g, ""); // retira os pontos e as v�rgulas, deixando somente n�meros
		campo2 = campo2.replace(/(\d{0})(\d)/, "$1$2");
		campo2 = parseInt(campo2); // converte para int

		campo3 = document.getElementById('Beneficiario').value;
		campo3 = campo3.replace(/\D/g, ""); // retira os pontos e as v�rgulas, deixando somente n�meros
		campo3 = campo3.replace(/(\d{0})(\d)/, "$1$2");
		campo3 = parseInt(campo3); // converte para int

		campo4 = document.getElementById('Normal').value;
		campo4 = campo4.replace(/\D/g, ""); // retira os pontos e as v�rgulas, deixando somente n�meros
		campo4 = campo4.replace(/(\d{0})(\d)/, "$1$2");
		campo4 = parseInt(campo4); // converte para int

		campo5 = document.getElementById('Promocional').value;
		campo5 = campo5.replace(/\D/g, ""); // retira os pontos e as v�rgulas, deixando somente n�meros
		campo5 = campo5.replace(/(\d{0})(\d)/, "$1$2");
		campo5 = parseInt(campo5); // converte para int

		// total de ingressos
		total = campo1 + campo2 + campo3 + campo4 + campo5;

		if (isNaN(total)) // se Não for n�mero
		{
			total = '';
		}
		else
		{
			total = (parseFloat(total)).toFixed(0); // armazena o resultado

			// formata para real
			total = total.replace(/\D/g, "");
			total = total.replace(/(\d+)(\d{3},\d{2})$/g, "$1.$2");

			var q = (total.length - 3) / 3; // quantidade caracteres
			var c = 0; // contador
			while (q > c)
			{
				c++;
				total = total.replace(/(\d+)(\d{3}.*)/, "$1.$2");
			}
			total = total.replace(/^(0+)(\d)/g, "$2");
			total = total;
		}
		document.getElementById('Numero').value = total;
		/* ========== FIM SOMA DE INGRESSOS ========== */


		/* ========== INÍCIO CALCULO DE INGRESSOS ========== */
		campo6 = document.getElementById('Normal_Uni').value;
		campo6 = campo6.replace(/\D/g, ""); // retira os pontos e as v�rgulas, deixando somente n�meros
		campo6 = campo6.replace(/(\d{0})(\d)/, "$1$2");
		campo6 = campo6.replace(/(\d)(\d{2})$/, "$1.$2"); // adiciona o ponto na casa decimal
		campo6 = parseFloat(campo6).toFixed(2); // converte para float e adiciona precis�o decimal

		campo7 = document.getElementById('Proporcional').value;
		campo7 = campo7.replace(/\D/g, ""); // retira os pontos e as v�rgulas, deixando somente n�meros
		campo7 = campo7.replace(/(\d{0})(\d)/, "$1$2");
		campo7 = campo7.replace(/(\d)(\d{2})$/, "$1.$2"); // adiciona o ponto na casa decimal
		campo7 = parseFloat(campo7).toFixed(2); // converte para float e adiciona precis�o decimal

		// vari�vel com o resultado
		total_normal      = parseFloat(campo6 * campo4).toFixed(2); // armazena o resultado
		total_promocional = parseFloat(campo7 * campo5).toFixed(2); // armazena o resultado


		// total de ingressos (normal + promocional)
		if (isNaN(total_normal) || isNaN(total_promocional)) // se Não for n�mero
		{
			total_geral = '';
		}
		else
		{
			total_geral = (parseFloat(total_normal) + parseFloat(total_promocional)).toFixed(2); // armazena o resultado

			// formata para real
			total_geral = total_geral.replace(/\D/g, "");
			total_geral = total_geral.replace(/(\d)(\d{2})$/, "$1,$2");
			total_geral = total_geral.replace(/(\d+)(\d{3},\d{2})$/g, "$1.$2");

			var q = (total_geral.length - 3) / 3; // quantidade caracteres
			var c = 0; // contador
			while (q > c)
			{
				c++;
				total_geral = total_geral.replace(/(\d+)(\d{3}.*)/, "$1.$2");
			}
			total_geral = total_geral.replace(/^(0+)(\d)/g, "$2");
			total_geral = 'R$ ' + total_geral;
		}
		document.getElementById('total').value = total_geral;


		// total de ingressos (normal)
		if (isNaN(total_normal)) // se Não for n�mero
		{
			total_normal = '';
		}
		else
		{
			// formata para real
			total_normal = total_normal.replace(/\D/g, "");
			total_normal = total_normal.replace(/(\d)(\d{2})$/, "$1,$2");
			total_normal = total_normal.replace(/(\d+)(\d{3},\d{2})$/g, "$1.$2");

			var q = (total_normal.length - 3) / 3; // quantidade caracteres
			var c = 0; // contador
			while (q > c)
			{
				c++;
				total_normal = total_normal.replace(/(\d+)(\d{3}.*)/, "$1.$2");
			}
			total_normal = total_normal.replace(/^(0+)(\d)/g, "$2");
			total_normal = 'R$ ' + total_normal;
		}
		document.getElementById('valor1').value = total_normal;


		// total de ingressos (promocional)
		if (isNaN(total_promocional)) // se Não for n�mero
		{
			total_promocional = '';
		}
		else
		{
			// formata para real
			total_promocional = total_promocional.replace(/\D/g, "");
			total_promocional = total_promocional.replace(/(\d)(\d{2})$/, "$1,$2");
			total_promocional = total_promocional.replace(/(\d+)(\d{3},\d{2})$/g, "$1.$2");

			var q = (total_promocional.length - 3) / 3; // quantidade caracteres
			var c = 0; // contador
			while (q > c)
			{
				c++;
				total_promocional = total_promocional.replace(/(\d+)(\d{3}.*)/, "$1.$2");
			}
			total_promocional = total_promocional.replace(/^(0+)(\d)/g, "$2");
			total_promocional = 'R$ ' + total_promocional;
		}
		document.getElementById('valor2').value = total_promocional;
		/* ========== FIM CALCULO DE INGRESSOS ========== */

    } // fecha fun��o somaIngressos()
</script>
