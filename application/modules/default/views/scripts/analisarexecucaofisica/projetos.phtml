<script type="text/javascript">
    $(document).ready(function(){
        $('.btn_enviar').click(function(){
            var tecnico = $(this).parent().parent().find('select').val(),
                pronac = $(this).attr('pronac'),
                nr = $(this).attr('nr');

            if(tecnico==''){
                $("#alerta").dialog("destroy");
                $("#alerta").html('Favor selecionar o t&eacute;cnico!');
                $("#alerta").dialog({
                    resizable: false,
                    title: 'Alerta!',
                    width:320,
                    height:160,
                    modal: true,
                    buttons : {
                        'OK' : function(){
                            $(this).dialog('close');
                        }
                    }
                });
                $('.ui-dialog-titlebar-close').remove();
            } else {
                $.ajax({
                    type: 'POST',
                    url: '<?php echo $this->url(array('controller' => 'analisarexecucaofisica', 'action' => 'encaminhar-relatorio'), '', true); ?>?idpronac=<?php echo $this->idPronac; ?>',
                    data: {
                        tecnico: tecnico,
                        pronac: pronac,
                        nr: nr
                    },
                    success: function(data){
                        if(data.resposta){
                            $("#alerta").dialog("destroy");
                            $("#alerta").html('<br /><div align="center"><img src="<?php echo $this->baseUrl().'/public/img/ajax.gif'; ?>"></div>');
                            $("#alerta").dialog({
                                resizable: false,
                                width:320,
                                height:120,
                                modal: true,
                                title: 'Carregando..'
                            });
                            $('.ui-dialog-titlebar-close').remove();
                            window.setTimeout('location.reload()', 1000);
                        } else {
                            alertaModal("Não foi poss�vel encaminhar o relat�rio.");
                        }
                    },
                    dataType : 'json'
                });
            }
        });

        $('.btn_finalizar').click(function(){
            var pronac = $(this).attr('pronac'),
                nr = $(this).attr('nr');

            $("#pergunta").dialog("destroy");
            $("#pergunta").html('Deseja realmente finalizar o relat�rio?');
            $("#pergunta").dialog({
                resizable: false,
                title: 'Alerta!',
                width:320,
                height:160,
                modal: true,
                buttons : {
                    'Não' : function(){
                        $("#pergunta").dialog('close');
                    },
                    'Sim' : function(){
                        $("#pergunta").dialog('close');
                        $.ajax({
                            type: 'POST',
                            url: '<?php echo $this->url(array('controller' => 'analisarexecucaofisica', 'action' => 'finalizar-relatorio'), '', true); ?>?idpronac=<?php echo $this->idPronac; ?>',
                            data: {
                                pronac: pronac,
                                nr: nr
                            },
                            success: function(data){
                                if(data.resposta){
                                    $("#alerta").dialog("destroy");
                                    $("#alerta").html('<br /><div align="center"><img src="<?php echo $this->baseUrl().'/public/img/ajax.gif'; ?>"></div>');
                                    $("#alerta").dialog({
                                        resizable: false,
                                        width:320,
                                        height:120,
                                        modal: true,
                                        title: 'Carregando..'
                                    });
                                    $('.ui-dialog-titlebar-close').remove();
                                    window.setTimeout('location.reload()', 1000);
                                } else {
                                    alertaModal("Não foi poss�vel finalizar o relat�rio.");
                                }
                            },
                            dataType : 'json'
                        });
                    }
                }
            });
            $('.ui-dialog-titlebar-close').remove();
        });

        $('.btn_localizar').click(function(){
            var pronac = $('#campoPesquisa').val();
            location.href="<?php echo $this->baseUrl()."/analisarexecucaofisica/projetos?pronac="; ?>"+pronac;
        });

    });

    function alertaModal(msg){
        $("#alerta").dialog("destroy");
        $("#alerta").html(msg);
        $("#alerta").dialog({
            resizable: false,
            title: 'Alerta!',
            width:330,
            height:170,
            modal: true,
            buttons : {
                'OK' : function(){
                    $(this).dialog('close');
                }
            }
        });
        $('.ui-dialog-titlebar-close').remove();
    }
</script>

