        <!-- ========== GRID NOME DA PROPOSTA ========== -->
        <script>
            function abrir_fechar(id_div){

                $('#'+id_div).slideToggle(400);

                $('#maiss'+id_div).toggle().focus();

                $('#menoss'+id_div).toggle().focus();

            }



            function EnviarArquivo()
            {

                // modal com os dados do dirigente
                $("#modalEnviarArquivo").dialog("destroy");
                $("#modalEnviarArquivo").dialog
                ({
                    width:550,
                    height:400,
                    EscClose:false,
                    modal:true
                    ,buttons:
                        {
                        'Enviar Arquivo':function()
                        {
                            selecionarOptions();
                            document.form.submit();
                            $(this).dialog('close'); // fecha a modal

                        },
                        'Cancelar':function()
                        {
                            $(this).dialog('close'); // fecha a modal
                            buscarDirigentes();
                        }
                    }
                });
                $('.ui-dialog-titlebar-close').remove();

            } // fecha func?o adicionar_dirigente_agente()

        </script>

